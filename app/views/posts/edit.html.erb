<h1>Edit this post!</h1>

<%= form_for @post do |f| %>

    <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div><br>

    <div>
        <%= f.label :content %>
        <%= f.text_field :content %>
    </div>



    <%= f.submit "Submit these changes!" %>

<% end %>

<%= link_to "Back to the list!", posts_path %>

