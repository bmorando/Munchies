<h1><em>Num Nom Munchies</em></h1>
<h2>Add a new post!</h2>

<%= form_for @post do |f| %>

    <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>

    <div>
        <%= f.file_field :image %>
        <%= f.label :content %>
        <%= f.text_field :content %>
    </div>





    <%= f.submit "Add new post!" %>

<% end %>

<%= link_to "Back to the list!", posts_path %>

